# letohd udev rules. Start daemon on insert, stop on remove
# TOH_VID = 0x4b4c, TOH_PID = 0x0005
#
KERNEL=="toh-core.0", ATTR{vendor}=="19276", ATTR{product}=="5", ACTION=="add", RUN+="/bin/systemctl start harbour-letohd"
KERNEL=="toh-core.0", ACTION=="remove", RUN+="/bin/systemctl stop harbour-letohd"
